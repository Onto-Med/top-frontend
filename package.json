    "quasar": "^2.7.7",
    "uuid": "^8.3.2",
    "vue": "^3.2.37",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.3"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.13.14",
    "@quasar/app-webpack": "^3.5.9",
    "@quasar/quasar-app-extension-testing": "^2.0.4",
