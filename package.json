    "@typescript-eslint/parser": "^5.28.0",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-vue": "^7.0.0"
  },
